<script>
    $('.carousel').carousel({
        interval: false
    });

    var $videoSrc;
    $(".mam ").click(function () {
        PlayVideos($(this))
    });
    function PlayVideos(ev) {
        $videoSrc = $(ev).attr("src");
        $videoSrc = $videoSrc.replace('?amp;showinfo=0', '').replace('&autoplay=1', '&amp;autoplay=1').replace('&amp;autoplay=1', '')
        document.getElementById("myModal").style.display = "block";
        var Arrysrc = document.getElementsByClassName("mySlides");

        for (var i = 0; i < length; i++) {
            var videolist = Arrysrc[i].lastElementChild.src;
            videolist = videolist.replace('?amp;showinfo=0', '').replace('&autoplay=1', '&amp;autoplay=1').replace('&amp;autoplay=1', '');
            if ($videoSrc == videolist) {

                $('#ifrmeID' + i).attr("src", $videoSrc + "?amp;showinfo=0&autoplay=1");
                $('#ifrmeID' + i).attr("allow", "autoplay");
                document.getElementsByClassName("mySlides")[i].style.display = "block";
                document.getElementsByClassName("nowplay")[i].style.display = "block";
                document.getElementsByClassName("hideIcon")[i].style.display = "none";

            }
            else {
                $('#ifrmeID' + i).attr("src", Arrysrc[i].lastElementChild.src.replace('&autoplay=1', '').replace('&amp;autoplay=1', ''));
                document.getElementsByClassName("mySlides")[i].style.display = "none";
                document.getElementsByClassName("nowplay")[i].style.display = "none";
                document.getElementsByTagName("span")[i].style.display = "block";

            }
        }
    }

    var slideIndex = 1;
    showSlides(slideIndex);
    var IsSlide = false;
    function plusSlides(n) {
        IsSlide = true;
        showSlides(slideIndex += n);
        IsSlide = false;
    }

    function currentSlide(n) {
        showSlides(slideIndex = n);
    }
    function showSlides(n) {
        debugger;
        var i;
        var slides = document.getElementsByClassName("mySlides");
        var dots = document.getElementsByClassName("demo");
        var captionText = document.getElementById("caption");
        if (n > slides.length) { slideIndex = 1 }
        if (n < 1) { slideIndex = slides.length }
        if (n == 1) {
            $("#tutorialPrev").css('display', 'none');
        }
        else {
            $("#tutorialPrev").css('display', 'block');
        } if (n == slides.length) {
            $("#tutorialNext").css('display', 'none');
        }
        else {
            $("#tutorialNext").css('display', 'block');
        }
        for (i = 0; i < slides.length; i++) {
            slides[i].style.display = "none";

        }
        slides[slideIndex - 1].style.display = "block";
        if (IsSlide == true) { PlayVideos($(slides[slideIndex - 1]).find("iframe")[0]) }
    }
    function closeModal() {
        debugger;
        document.getElementById("myModal").style.display = "none";
        var NewArrysrc = document.getElementsByClassName("mySlides");
        for (var i = 0; i < length; i++) {
            $('#ifrmeID' + i).attr("src", NewArrysrc[i].lastElementChild.src.replace('&autoplay=1', '').replace('&amp;autoplay=1', ''));
        }
    }
</script>
